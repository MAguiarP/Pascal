Program Expressoes;

Var N1,N2,N3,N4, Conta: integer; 

Begin
  writeln('Digite N1');
  Readln(N1);                                                            
  writeln('Digite N2');
  Readln(N2);
  writeln('Digite N3');
  Readln(N3);                                                            
  writeln('Digite N4');
  Readln(N4);
  Conta :=N1 + N2 + N3 + N4;
  writeln('A conta é: ' , Conta);
End.      